                                </Button>
                            </Border>

                            <Border Grid.Column="9" Padding = "5" >
                                <Border.Background>
                                    <SolidColorBrush Color="Black" Opacity="0"/>
                                </Border.Background>
                                <Button x:Name="twOne_button" FontSize="20" Content="Ф" Click="One_button_Click" BorderBrush="{x:Null}">
                                    <Button.Background>
                                        <SolidColorBrush Color="Black" Opacity="0"/>
                                    </Button.Background>
                                </Button>
                            </Border>

                            <Border Grid.Column="10" Padding = "5" >
